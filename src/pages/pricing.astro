---
import Base from "@/layouts/Base.astro";
import PageHeader from "@/layouts/components/PageHeader.astro";
import PricingData from "@/layouts/jsx-components/Pricing.jsx";
import Cta from "@/layouts/partials/Cta.astro";
import { getEntry } from "astro:content";

const pricing = await getEntry("pricing", "index");
const { title, description } = pricing.data;
const { data } = pricing;
---

<Base title={title} description={description}>
  <PageHeader title={title} />
  <PricingData data={data} client:load />
  <Cta />
</Base>
